	<section id="cart_items">
		<div class="container">
			<div class="breadcrumbs">
				<ol class="breadcrumb">
				  <li><a>후원관리</a></li>
				  <li class="active">나의 후원목록</li>
				</ol>
				
			</div>
			<div class="table-responsive cart_info">
				<table class="table table-condensed">
					<thead>
						<tr class="cart_menu">
							<td class="image">사진</td>
							<td class="description">프로젝트</td>
							<td class="price">금액</td>
							<td class="quantity">상태</td>
							<td></td>
						</tr>
					</thead>
					<tbody>
					<c:forEach var="support" items="${supports}" begin="0" step="1" varStatus="status" >
						<tr>
							<td class="cart_product">
								<a href="/goldcow/project/projectdetail?projectNo=${support.project.no}"><img src="../resources/${support.project.no }.jpg" alt="" width = "110px" height = "110px"></a>
							</td>
							<td class="cart_description">
								<h4><a href="/goldcow/project/projectdetail?projectNo=${support.project.no}">${support.project.name}</a></h4>
							</td>
							<td class="cart_price">
							<p>${support.amount}</p>
							</td>
							<td class="cart_quantity">
							<c:if test="${support.project.state eq 0}">
							<p>진행중<a href = "supportcancel.do?no=${support.no}"><button type = "button">취소</button></a></p>
							</c:if>
							<c:if test="${support.project.state eq 1}">
							<p>성공</p>
							</c:if>							
							</td>
							</tr>
							</c:forEach>
					</tbody>
				</table>
				
			</div>
														<ul class="pagination">
											
											<c:if test="${pm.page ne 1}">
											<li><a href="/goldcow/support/mysupportlist?page=${pm.page-1 }">&laquo;</a></li>
											</c:if>
											<c:forEach var="i" begin="${pm.startPage }" end="${pm.endPage }" step="1" >
											<c:if test="${i eq pm.page}">
							<li class="active"><a href="/goldcow/support/mysupportlist?page=${i }"> ${i }</a></li>
							</c:if>
																		<c:if test="${i ne pm.page}">
							<li><a href="/goldcow/support/mysupportlist?page=${i }"> ${i }</a></li>
							</c:if>
							</c:forEach>
							
							<c:if test="${pm.page ne pm.pageEntity}">
							<li><a href="/goldcow/support/mysupportlist?page=${pm.page+1 }">&raquo;</a></li>
							</c:if>
							
						</ul>
		</div>
	</section> <!--/#cart_items-->
